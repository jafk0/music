.search-wrapper {
  padding: 10rpx 30rpx;
  position: relative;
}
.search-wrapper .search {
  border-radius: 5px;
  border: 2px solid #333;
  font-size: 12px;
}
.placeholder {
  font-size: 12px;
  text-align: center;
}

.list-title{
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 15px;
  font-weight: bold;
  color: #665b3c;
  width: 100%;
}


.rank .toplist {
  height: 100%;
  overflow: hidden;
}
.rank .toplist .item {
  display: flex;
  margin: 0 50rpx;
  margin-top: 40rpx;
  height: 200rpx;
  background: #f3f3f3;
  border-radius: 30rpx;
}
.rank .toplist .item .icon image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 30rpx;
}
.rank .toplist .item .icon {
  width: 200rpx;
  height: 200rpx;
}
.rank .toplist .item .songlist {
  padding: 20rpx 40rpx 0 40rpx;
  height: 200rpx;
  overflow: hidden;
  color: #7b7b7b;
  font-size: 12px;
}
.rank .toplist .item .songlist .song {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  line-height: 50rpx;
}
